.container-fluid.row.mainNavBar.fixed-top.w-100.navbar-expand-lg
    //Navbar
    .col-2.d-flex.justify-content-center
        a(href="/")
            img(src="logo.png" alt="AN BINH logo" width="50" height="50")
    .col-7.d-flex.justify-content-center
        h1 HỆ THỐNG TIÊM CHỦNG AN BÌNH
    .col-3.d-flex.justify-content-center
        ul(id="nav-list").nav.align-items-center
            if user
                li.nav-item.dropdown
                    a(class="contact" href="#")= user.userName
                    div(class="dropdown-content")
                        a(class="dropdown-item" href="/logOut") ĐĂNG XUẤT
            else
                li.nav-item
                    a(class="contact" href="/logIn") ĐĂNG NHẬP
                li.nav-item
                    a(class="contact" href="/signUp") ĐĂNG KÝ      
        div.col-12(id="search-bar")
            i(class="fas fa-search fa-lg" onclick="myFunction()")
            input(type="text" id="searchBar" placeholder="Search Here ..... !")
            div(id="search_List")
                ul(id="dishesList")
.page-wrapper.bg-blue.p-t-100.p-b-100.font-robo
    .wrapper.wrapper--w680
        .card.card-1
            .card-heading.d-flex.justify-content-center
                .img-container
                    img(src="logo.png" alt="AN BINH logo" width="300" height="300")
            .card-body
                h2.title PHIẾU ĐĂNG KÝ MUA VACCINE
                h5.title Thông tin khách hàng
                form(method='POST')
                    .input-group
                        input.input--style-1(type='text' placeholder='HỌ VÀ TÊN' name='name')
                    .row.row-space
                        .col-8
                            .input-group
                                input.js-datepicker(type='text' placeholder='NGÀY SINH' name='birthday')
                                i.zmdi.zmdi-calendar-note.input-icon.js-btn-calendar
                        .col-4
                            .input-group
                                .rs-select2.js-select-simple.select--no-search
                                    select(name='gender')
                                        option(disabled='disabled' selected='selected') GIỚI TÍNH
                                        option Nam
                                        option Nữ
                                    .select-dropdown
                    .input-group
                        .rs-select2.js-select-simple.select--no-search
                            input.input--style-1(type='text' placeholder='SỐ ĐIỆN THOẠI' name='phone')
                    .input-group
                        .rs-select2.js-select-simple.select--no-search
                            input.input--style-1(type='text' placeholder='ĐỊA CHỈ' name='address')
                    h5.title Thông tin Vaccine
                    .row.row-space
                        .col-12
                            .input-group
                                .rs-select2.js-select-simple.select--no-search
                                    select(name='vaccine')
                                        option(disabled='disabled' selected='selected') CHỌN VACCINE MUỐN ĐẶT MUA
                                        option VACCINE 01
                                        option VACCINE 02
                                        option VACCINE 03
                                    .select-dropdown
                    .p-t-20
                        button.btn.btn--radius.btn--green(type='submit') Submit
